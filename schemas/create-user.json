{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "required": [ "username", "email" ],
    "properties": {
        "username": { "type": "string", "minLength": 3 },
        "email": { "type": "string", "format": "email" },
        "age": { "type": "integer", "minimum": 0 }
    }
}
