{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "required": [ "id" ],
    "properties": {
        "id": { "type": "string", "pattern": "^[a-f0-9\\-]{36}$" },
        "username": { "type": "string" },
        "email": { "type": "string", "format": "email" },
        "age": { "type": "integer" }
    }
}
